{
  "AllowedHosts": "*",
  "Analytics": {
    "GoogleTag": "TODO"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.BrowserConsole" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "BrowserConsole",
        "Args": {
          "OutputTemplate": "{Timestamp:HH:mm:ss}-{Level:u3}{Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "TrianglePegGameSolver.Web"
    }
  }
}